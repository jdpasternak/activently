<div class="container">
  <h4>What's up buddy!</h4>
</div>

<div class="container">
  <div class="row">
    <div class="col s12 m5">
      <div class="card-panel">
        <form class="login-form">
          <div>
            <label for="email-login">Enter your email:</label>
            <input type="text" id="email-login" />
          </div>
          <div>
            <label for="password-login">Enter your password:</label>
            <input type="password" id="password-login" />
          </div>

          <div class="row">
            <div class="col s6">
              <button
                class="btn waves-effect waves-light"
                type="submit"
                name="action"
              >Login!
                <i class="material-icons right">send</i>
              </button>
            </div>
            <div class="col s6">
              <button
                class="btn waves-effect waves-light"
                type="submit"
                name="action"
              >Not A Member?
                <i class="material-icons right"></i>
              </button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
</div>
