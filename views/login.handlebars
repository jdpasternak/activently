<div class="container">
  <h4>What's up buddy!</h4>
</div>

<div class="container">
  <div class="row">
    <div class="col s12">
      <div class="card">
        <div class="card-content">
          <span class="card-title black-text">Login</span>
          <form class="login-form">
            <div class="input-field">
              <label for="email-login">Enter your email:</label>
              <input type="text" id="email-login" class="validate" />
            </div>
            <div class="input-field">
              <label for="password-login">Enter your password:</label>
              <input type="password" id="password-login" />
            </div>

            <div class="row">
              <div class="col s6">
                <button
                  class="btn waves-effect waves-light"
                  type="submit"
                  name="action"
                >Login!
                  <i class="material-icons right">send</i>
                </button>
              </div>
              <div class="col s6">
                <a
                  href="/signup"
                  class="btn waves-effect waves-light btn-flat center"
                >Not A Member?
                  <i class="material-icons right"></i>
                </a>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="/javascript/login.js"></script>
